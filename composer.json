{
    "name": "sudippalash/lang-maker",
    "description": "Laravel package for Language Make and Edit",
    "keywords": [
        "Sudip",
        "Laravel Language Make and Edit",
        "Language Make and Edit",
        "Laravel"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sudip Palash",
            "email": "palash.sudip@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.64|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
          "Sudip\\LangMaker\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sudip\\LangMaker\\Providers\\AppServiceProvider",
                "Sudip\\LangMaker\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
